{
  "AWSEBDockerrunVersion": "3",
  "containerDefinitions": [
    {
      "name": "client",
      "image": "shan1500/complex-client",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ]
    },
    {
      "name": "server",
      "image": "shan1500/complex-server",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 5000,
          "containerPort": 5000
        }
      ]
    },
    {
      "name": "worker",
      "image": "shan1500/complex-worker",
      "essential": false,
      "memory": 128
    }
  ]
}